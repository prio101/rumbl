<div class="jumbotron">
  <h2>Welcome to Rumbl.io</h2>
  <p class="lead">
    Rumbl It Out
  </p>
</div>

<h1>Listing Users</h1>

<table class="table">
  <%= for user <- @users do %>
    <tr>
      <td><b><%= first_name(user) %></b>(<%= user.id %>)</td>
      <td><%= link "View", to: user_path(@conn, :show, user.id) %></td>
    </tr>
  <% end %>
</table>